{
  "grammars": [
    {
      "name": "sml",
      "camelcase": "SML",
      "scope": "source.sml",
      "path": ".",
      "file-types": [
        "sml", "sig", "fun"
      ],
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm",
      "injection-regex": "^sml$"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Standard ML grammar for tree-sitter",
    "authors": [
      {
        "name": "Byron Zhong",
        "email": "zhongbangyuan@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/zbyrn/tree-sitter-sml"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true
  }
}
